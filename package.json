{
  "name": "outdated-plus",
  "version": "1.2.0",
  "description": "CLI tool to check npm outdated packages with publication dates",
  "main": "dist/index.js",
  "bin": {
    "outdated-plus": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:run": "vitest run",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsc -w",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src tests",
    "format:check": "prettier --check src tests"
  },
  "keywords": [
    "npm",
    "outdated",
    "dependencies",
    "cli",
    "dates",
    "package-management"
  ],
  "author": "AlexF090",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexF090/outdated-plus.git"
  },
  "bugs": {
    "url": "https://github.com/AlexF090/outdated-plus/issues"
  },
  "homepage": "https://github.com/AlexF090/outdated-plus#readme",
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
